{
  "name": "dasscom",
  "version": "1.0.0",
  "main": "src/main/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5173",
    "electron": "cross-env NODE_ENV=development electron .",
    "electron-dev": "concurrently -k \"npm run dev\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development npm run electron\"",
    "dist": "electron-builder"
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "electron-log": "^5.4.3",
    "exceljs": "^4.4.0",
    "ip": "^2.0.1",
    "pin": "^0.3.0",
    "ping": "^0.4.4",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.1",
    "xlsx": "^0.18.5"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "vite": "^7.1.7",
    "wait-on": "^9.0.1"
  },
  "build": {
    "files": [
      "dist/**",
      "src/**",
      "preload/**",
      "api/**",
      "utils/**"
    ],
    "asarUnpack": [
      "binaries/**"
    ],
    "extraResources": [
      {
        "from": "binaries",
        "to": "binaries"
      }
    ],
    "npmRebuild": false,
  "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "signAndEditExecutable": false
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  }
}
